                                    </fragment>
                                    <fragment content="竈門家">
                                        <attributes>
                                            <color key="NSBackgroundColor" red="0.99519230769230771" green="0.99038461538461542" blue="1" alpha="0.0" colorSpace="custom" customColorSpace="displayP3"/>
